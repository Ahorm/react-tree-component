    "react-dom": "^0.14.2",
    "whatwg-fetch": "^0.10.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babelify": "7.2.0",
    "browserify": "^12.0.1",
    "ecstatic": "^1.2.0",
    "parallelshell": "^2.0.0",
    "standard": "^5.3.1",
